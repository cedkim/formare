{
  "name": "formare-docs",
  "version": "1.0.0",
  "author": "David Kurnia Kristiadi <davidkurniakristiadi@gmail.com>",
  "homepage": "https://dkk94.github.io/formare",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "node-sass": "^4.9.0",
    "prismjs": "^1.14.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "css:watch": "npm run css:build -- --watch",
    "css:build": "node-sass --output-style expanded ./src/scss/formare-docs.scss ./src/css/formare-docs.css",
    "js:watch": "react-scripts start",
    "js:build": "react-scripts build",
    "start": "npm run css:watch | npm run js:watch",
    "build": "npm run js:build && npm run css:build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "gh-pages": "^2.0.0"
  }
}
